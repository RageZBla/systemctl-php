        "php": "^7.3",
        "symfony/process": "^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "codeclimate/php-test-reporter": "dev-master",
        "slevomat/coding-standard": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "icanhazstring\\SystemCtl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "icanhazstring\\SystemCtl\\Test\\": "test/"
        }
    },
